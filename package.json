{
  "name": "svg-unique-id",
  "version": "1.0.0",
  "description": "Assigns unique IDs to SVG elements and their children, preventing HTML validation errors",
  "main": "/out/extension.js",
  "scripts": {
    "start": "nodemon --exec \"npm run build\" --watch src",
    "build": "tsc"
  },
  "keywords": [
    "svg",
    "unique-id",
    "validation"
  ],
  "author": "Sam Lewis McEwan",
  "license": "MIT",
  "dependencies": {
    "punycode.js": "^2.3.1",
    "vsce": "^2.15.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "typescript": "^5.3.3",
    "vscode": "^1.1.37"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onDocumentChange",
    "onDocumentOpen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/samlmcewan/svg-unique-id"
  }
}
